<%- include("template/header", {tituloWeb: 'CrearMascota' }) %>
<div class="container">
  <h1>Crear</h1>
  <form action="/mascotas" method="post">
    <input
      type="text"
      placeholder="Agregar Nombre"
      class="form-control my-2"
      name="nombre"
    />
    <input
      type="text"
      placeholder="Agregar Descripcion"
      class="form-control my-2"
      name="descripcion"
    />
    <button class="btn btn-success btn-block" type="submit">Agregar</button>
  </form>
</div>
